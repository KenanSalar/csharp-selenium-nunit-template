{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "SeleniumTraining": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithEnvironmentUserName"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "SeleniumTrainingTests"
    }
  },
  "VisualTestSettings": {
    "BaselineDirectory": "ProjectVisualBaselines",
    "AutoCreateBaselineIfMissing": true,
    "WarnOnAutomaticBaselineCreation": true
  },
  "TestFrameworkSettings": {
    "HighlightElementsOnInteraction": false,
    "HighlightDurationMs": 0
  },
  "SeleniumGrid": {
    "Enabled": true,
    "Url": "http://selenium-hub:4444"
  },
  "ChromeBrowserOptions": {
    "Headless": true,
    "TimeoutSeconds": 0,
    "WindowWidth": 1920,
    "WindowHeight": 1080,
    "ChromeHeadlessArgument": "--headless=new",
    "ChromeArguments": [
      "--no-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu",
      "--disable-extensions",
      "--disable-popup-blocking",
      "--ignore-certificate-errors",
      "--start-maximized",
      "--disable-infobars",
      "--password-store=basic",
      "--disable-sync",
      "--no-first-run",
      "--no-default-browser-check",
      "--disable-features=PasswordManagerInterstitial"
    ],
    "LeaveBrowserOpenAfterTest": false,
    "UserProfilePreferences": {
      "profile.password_manager_leak_detection": false,
      "credentials_enable_service": false,
      "profile.password_manager_enabled": false,
      "profile.default_content_setting_values.password_protection": 2,
      "autofill.profile_enabled": false,
      "autofill.credit_card_enabled": false,
      "profile.default_content_settings.popups": 2
    }
  },
  "EdgeBrowserOptions": {
    "Headless": true,
    "TimeoutSeconds": 0,
    "WindowWidth": 1920,
    "WindowHeight": 1080,
    "ChromeHeadlessArgument": "--headless=new",
    "ChromeArguments": [
      "--no-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu",
      "--disable-extensions",
      "--start-maximized"
    ],
    "LeaveBrowserOpenAfterTest": false,
    "UserProfilePreferences": {
      "credentials_enable_service": false,
      "profile.password_manager_enabled": false
    }
  },
  "BraveBrowserOptions": {
    "Headless": true,
    "TimeoutSeconds": 0,
    "WindowWidth": 1920,
    "WindowHeight": 1080,
    "ChromeHeadlessArgument": "--headless=new",
    "ChromeArguments": [
      "--no-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu",
      "--disable-extensions",
      "--disable-popup-blocking",
      "--ignore-certificate-errors"
    ],
    "LeaveBrowserOpenAfterTest": false
  },
  "FirefoxBrowserOptions": {
    "Headless": false,
    "TimeoutSeconds": 0,
    "WindowWidth": 1920,
    "WindowHeight": 1080,
    "FirefoxHeadlessArgument": "--headless",
    "FirefoxArguments": [
      "--width=1920",
      "--height=1080",
      "--disable-gpu",
      "--private",
      "--disable-application-cache"
    ],
    "LeaveBrowserOpenAfterTest": false,
    "FirefoxProfilePreferences": {
      "signon.rememberSignons": false,
      "dom.webnotifications.enabled": false,
      "datareporting.healthreport.uploadEnabled": false,
      "browser.startup.homepage_override.mstone": "ignore"
    }
  }
}